  "dependencies": {
    "@material-ui/core": "^4.11.1",
    "@material-ui/icons": "^4.9.1",
    "bs58": "^4.0.1",
    "copy-to-clipboard": "^3.3.1",
    "crypto-js": "^4.0.0",
    "date-and-time": "^0.14.1",
    "i18next": "^19.8.4",
    "i18next-browser-languagedetector": "^6.0.1",
    "int64-buffer": "^0.99.1007",
    "js-base64": "^3.6.0",
    "pretty-bytes": "^5.4.1",
    "qrcode.react": "^1.0.0",
    "react": "^16.14.0",
    "react-beforeunload": "^2.4.0",
    "react-dom": "^16.14.0",
    "react-i18next": "^11.7.3",
